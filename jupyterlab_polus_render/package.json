{
  "name": "jupyterlab_polus_render",
  "version": "1.0.0",
  "description": "A Jupyterlab extension for Polus Render.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/PolusAI/jupyterlab-extensions/tree/master/jupyterlab_polus_render",
  "bugs": {
    "url": "https://github.com/PolusAI/jupyterlab-extensions/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Akshat Saini, Jeff Chen",
    "email": "akshat.saini@axleinfo.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/PolusAI/jupyterlab-extensions"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:nbextension && jlpm run build:labextension:dev",
    "build:prod": "jlpm run build:lib && jlpm run build:nbextension && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:nbextension": "webpack",
    "clean": "jlpm run clean:lib && jlpm run clean:nbextension && jlpm run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf jupyterlab_polus_render/labextension",
    "clean:nbextension": "rimraf jupyterlab_polus_render/nbextension/static/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "prepack": "jlpm run build:lib",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3 || ^4 || ^5 || ^6",
    "@labshare/polus-render": "^0.1.4-test",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@math.gl/core": "^3.5.7",
    "assert": "^2.0.0",
    "bootstrap": "^5.0.1",
    "crypto-browserify": "^3.12.0",
    "deck.gl": "~8.6.7",
    "document-register-element": "^1.7.2",
    "geotiff": "^2.0.5",
    "html2canvas": "^1.1.5",
    "https-browserify": "^1.0.0",
    "immutable": "^4.0.0",
    "immutable-rxjs": "^1.0.3",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "oidc-client": "^1.11.5",
    "os-browserify": "^0.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.3",
    "quill": "^1.3.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^11.2.3",
    "react-router-dom": "^5.2.0",
    "rxjs": "~6.6.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tslib": "^2.3.0",
    "url": "^0.11.0",
    "zone.js": "~0.14.3",
    "zustand": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@jupyter-widgets/base-manager": "^1.0.7",
    "@jupyterlab/builder": "^4.0.11",
    "@lumino/application": "^2.3.0",
    "@lumino/widgets": "^2.3.1",
    "@types/jest": "^29.5.11",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "acorn": "^8.11.3",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "~5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "jupyterlab_polus_render/labextension/",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
